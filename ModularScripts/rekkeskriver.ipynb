    "def a(n): # eksplisitt\n",
    "    return 1 + (n-1) * ((n*(n+1))/2)\n",
    "\n",
    "def a(n): # rekursiv\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    return a(n-1) + (n*(n+1))/2\n",
    "\n",
    "for n in range(1, 7):\n",
    "    print(a(n))"
